{
  "name": "ffmpeg_destination_demo",
  "group": "destination",
  "solution": "custom",
  "persistent": true,
  "autoStart": false,
  "detectionSensitivity": "Medium",
  "additionalParams": {
    "input": {
      "FILE_PATH": "/opt/edge_ai_api/videos/test.mp4",
      "RESIZE_RATIO": "1.0"
    },
    "output": {
      "FF_DES_URI": "rtmp://localhost:1935/live/ffmpeg_output",
      "FF_DES_CODEC": "libx264"
    }
  },
  "pipeline": [
    {
      "nodeType": "file_src",
      "nodeName": "file_src_ff_des_demo",
      "parameters": {
        "file_path": "${FILE_PATH}",
        "channel": "0",
        "resize_ratio": "${RESIZE_RATIO}"
      }
    },
    {
      "nodeType": "osd",
      "nodeName": "osd_ff_des_demo",
      "parameters": {
        "font_path": ""
      }
    },
    {
      "nodeType": "ff_des",
      "nodeName": "ff_des_demo",
      "parameters": {
        "channel": "0",
        "uri": "${FF_DES_URI}",
        "codec": "${FF_DES_CODEC}"
      }
    }
  ]
}

