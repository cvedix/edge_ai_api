{
  "solutionId": "pose_estimation_trt",
  "solutionName": "Pose Estimation (TensorRT YOLOv8)",
  "solutionType": "pose_estimation",
  "isDefault": false,
  "pipeline": [
    {
      "nodeType": "file_src",
      "nodeName": "file_src_{instanceId}",
      "parameters": {
        "file_path": "${FILE_PATH}",
        "channel": "0",
        "resize_ratio": "${RESIZE_RATIO}"
      }
    },
    {
      "nodeType": "trt_yolov8_pose_detector",
      "nodeName": "pose_detector_{instanceId}",
      "parameters": {
        "model_path": "${POSE_MODEL_PATH}"
      }
    },
    {
      "nodeType": "pose_osd",
      "nodeName": "pose_osd_{instanceId}",
      "parameters": {}
    },
    {
      "nodeType": "screen_des",
      "nodeName": "screen_des_{instanceId}",
      "parameters": {
        "channel": "0",
        "enabled": "${ENABLE_SCREEN_DES}"
      }
    },
    {
      "nodeType": "rtmp_des",
      "nodeName": "rtmp_des_{instanceId}",
      "parameters": {
        "rtmp_url": "${RTMP_URL}",
        "channel": "0"
      }
    }
  ],
  "defaults": {
    "detectorMode": "SmartDetection",
    "detectionSensitivity": "0.7",
    "sensorModality": "RGB",
    "RESIZE_RATIO": "1.0",
    "ENABLE_SCREEN_DES": "true"
  }
}

